<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="275.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.gui.controllers.editController.AddUserController">
   <children>
      <Label layoutX="225.0" layoutY="38.0" text="Select User:">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ComboBox fx:id="users" layoutX="174.0" layoutY="101.0" prefHeight="37.0" prefWidth="253.0" />
      <Button fx:id="addBtn" layoutX="174.0" layoutY="195.0" mnemonicParsing="false" onAction="#save" prefHeight="51.0" prefWidth="96.0" text="Add">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="cancelBtn" layoutX="331.0" layoutY="195.0" mnemonicParsing="false" onAction="#cancel" prefHeight="51.0" prefWidth="96.0" text="Cancel">
